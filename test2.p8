pico-8 cartridge // http://www.pico-8.com
version 16
__lua__

function _init()
	w = 128 // width of the game map
	h = 128 // height of the game map
	
	music(0)
	
	placeholder = 0
	
	merchant = {}
	merchant.sprite = 4
	merchant.x = 600
	merchant.y = 456
	
	keycounter = 0
	
	questone = {}
	questone.sprite = 68
	questone.x = 100
	questone.y = 150
	
	questoneboolean = false
	questonecounter = 0
	coconutsrecieved = 0
	
	coconut = {}
	coconut.sprite = 116
	coconut.x = 144
	coconut.y = 384
	
	questtwo = {}
	questtwo.sprite = 84
	questtwo.x = 696
	questtwo.y = 224
	
	questtwoboolean = false
	
	player = {}
	player.x = 64
	player.y = 64
	player.sprite = 1
	player.speed = 3
	player.moving = false
	player.moved_this_frame = false
	player.frame = 0
	player.step = 0
	player.steptwo = 0
	player.stepthree = false
	
	player_up = 17
	player_down = 1
	player_left = 49
	player_right = 33

	array_down = {0, 1, 2}
	array_up = {16, 17, 18}
	array_right = {32, 33, 34}
	array_left = {48, 49, 50}
	
	player.cm = true // collide with map tiles?
	player.cw = false // collide with world bounds?
end

function animate(sprite)
    player.moved_this_frame = true
	
	if(player.step % 3 == 0) then 
		player.frame += 1 
	end
	if(player.frame > 5) then
		
		if player.steptwo == 3 then
			player.stepthree = true
		end
		
		if player.steptwo == 1 then
			player.stepthree = false
		end
		
		if player.stepthree == false then
			player.steptwo += 1
		end
		
		if player.stepthree == true then
			player.steptwo -= 1
		end
		
		if sprite == 49 then
			player.sprite = array_left[player.steptwo]
		end
		
		if sprite == 33 then
			player.sprite = array_right[player.steptwo]
		end
		
		if sprite == 17 then
			player.sprite = array_up[player.steptwo]
		end
		
		if sprite == 1 then
			player.sprite = array_down[player.steptwo]
		end

		player.frame = 0
	end
end

function cmap(o)
	local ct=false
	local cb=false

	// if colliding with map tiles
	if(o.cm) then
		local x1=o.x/8
		local y1=o.y/8
		local x2=(o.x+7)/8
		local y2=(o.y+7)/8
		local a=fget(mget(x1,y1),0)
		local b=fget(mget(x1,y2),0)
		local c=fget(mget(x2,y2),0)
		local d=fget(mget(x2,y1),0)
		ct=a or b or c or d
	end
	
	// if colliding world bounds
	if(o.cw) then
		cb=(o.x<0 or o.x+8>w or o.y<0 or o.y+8>h)
	end

	return ct or cb
end

function move(player)
	player.moved_this_frame = false

	local lx =  player.x // last x
	local ly = player.y // last y

	// move left
	if btn(0) then
		if not player.moving then 
			player.sprite = player_left
		end
		
		player.x -= player.speed
		animate(player_left)
	end
	
	// move right
	if btn(1) then
		if not player.moving then 
			player.sprite = player_right
		end
		
		player.x += player.speed
		animate(player_right)
	end
	
	// move up
	if btn(2) then
		if not player.moving then 
			player.sprite = player_up
		end

		player.y -= player.speed
		animate(player_up)
	end
	
	// move down
	if btn(3) then
		if not player.moving then 
			player.sprite = player_down
		end
		
		player.y += player.speed
		animate(player_down)
	end 
	
	// press x
	if btn(4) then
		placeholder = 0
	end
	
	// press y
	if btn(5) then
		placeholder = 0
	end
	
	if player.moved_this_frame then
		player.moving = true
	else
		player.moving = false
	
		if (player.sprite == 48 or player.sprite == 49 or player.sprite == 50) then
			player.sprite = 49
		end
		
		if (player.sprite == 32 or player.sprite == 33 or player.sprite == 34) then
			player.sprite = 33
		end
		
		if (player.sprite == 16 or player.sprite == 17 or player.sprite == 18) then
			player.sprite = 17
		end
		
		if (player.sprite == 0 or player.sprite == 1 or player.sprite == 2) then
			player.sprite = 1
		end
	end

	// collision, move back
	if cmap(player) then
		player.x = lx 
		player.y = ly
	end
end

function text_and_quests()
	
	// Game beginning text
	if player.x < 120 and player.y < 90 then
		print("welcome to island rescue!", player.x - 49, player.y - 56, 8)
		print("find the merchant and", player.x - 42, player.y + 49, 8)
		print("unlock his cell to save him!", player.x - 56, player.y + 56, 8)
	end
	
	// Game end text with merchant
	if player.x >= merchant.x and player.x <= merchant.x + 16 and player.y >= merchant.y and player.y <= merchant.y + 16 then
		print("you found the merchant!", player.x - 56, player.y - 56, 8)
		print("he dabs and says thanks!", player.x - 56, player.y - 49, 8)
	end
	
	// Quest one
	if player.x >= questone.x - 12 and player.x <= questone.x + 12 and player.y >= questone.y - 12 and player.y <= questone.y + 12 then
		if questonecounter < 3 then
			print("hey kid, if you bring me three", player.x - 60, player.y - 56, 8)
			print("coconuts located south of here", player.x - 60, player.y - 49, 8)
			print("i'll give you this key", player.x - 42, player.y - 42, 8)
			questoneboolean = false
			coconutsrecieved = questonecounter
			print("coconuts handed in: "..coconutsrecieved, player.x - 60, player.y + 56, 8)
		end
		
		if questonecounter >= 3 then
			print("thanks for the coconuts kid", player.x - 54, player.y - 56, 8)
			print("you have obtained a key", player.x - 46, player.y - 42, 8)
			coconutsrecieved = questonecounter
			print("coconuts handed in: "..coconutsrecieved, player.x - 60, player.y + 56, 8)
		end
	end
	
	if player.x >= coconut.x - 8 and player.x <= coconut.x + 16 and player.y >= coconut.y - 8 and player.y <= coconut.y + 8 then
		if questoneboolean == false then
			questonecounter += 1
			questoneboolean = true
		end
		print("coconut count: "..questonecounter, player.x - 60, player.y - 56, 8)
	end
	
	// Quest two
	if player.x >= questtwo.x - 12 and player.x <= questtwo.x + 12 and player.y >= questtwo.y - 12 and player.y <= questtwo.y + 12 then
		if questtwoboolean == false then
			print("hey kid, i lost my coconut in", player.x - 60, player.y - 56, 8)
			print("this maze over here, go find", player.x - 56, player.y - 49, 8)
			print("it and i'll give you this key", player.x - 56, player.y - 42, 8)
			
			questoneboolean = false
			print("coconut returned: "..(questtwoboolean and 'true' or 'false'), player.x - 60, player.y + 56, 8)
		end
	end
	
end

function _update()
	move(player)
end

function _draw()
 cls()
	map(0, 0, 0, 0, 128, 64)
	spr(merchant.sprite, merchant.x, merchant.y)
	spr(questone.sprite, questone.x, questone.y)
	spr(questtwo.sprite, questtwo.x, questtwo.y)
	spr(coconut.sprite, coconut.x, coconut.y)
	spr(player.sprite, player.x, player.y)
	
	camera(player.x - 64, player.y - 64)
	
	text_and_quests()
end


