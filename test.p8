pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
player = {}
player.x = 50
player.y = 50
player.sprite = 1
player.speed = 2
player.moving = false

player_up = 17
player_down = 1
player_left = 49
player_right = 33

function move()
	player.moving = true
	
	if (player.sprite == 48 or player.sprite == 49 or player.sprite == 50) then
		if player.sprite 48 then
			player.sprite += 1
		end
		
	end
	
	
	//player.sprite += 1
	
	//if player.sprite > player.sprite + 1 then
	//	player.sprite = player.sprite - 3
	//end
end

function _update()
	player.moving = false

	// move left
	if btn(0) then
		player.sprite = player_left
		player.x -= player.speed
		move()
	end
	
	// move right
	if btn(1) then
		player.sprite = player_right
		player.x += player.speed
		move()
	end
	
	// move up
	if btn(2) then
		player.sprite = player_up
		player.y -= player.speed
		move()
	end
	
	// move down
	if btn(3) then
		player.sprite = player_down
		player.y += player.speed
		move()
	end 
	
	if not player.moving then
		if (player.sprite == 48 or player.sprite == 49 or player.sprite == 50) then
			player.sprite = 49
		end
		
		if (player.sprite == 32 or player.sprite == 33 or player.sprite == 34) then
			player.sprite = 33
		end
		
		if (player.sprite == 16 or player.sprite == 17 or player.sprite == 18) then
			player.sprite = 17
		end
		
		if (player.sprite == 0 or player.sprite == 1 or player.sprite == 2) then
			player.sprite = 1
		end
	end
end

function _draw()
	cls()
	spr(player.sprite, player.x, player.y)
end


